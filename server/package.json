{
  "name": "spa_tasks",
  "version": "0.0.0",
  "scripts": {
    "build": "node build.js",
    "lint": "tslint --project \"tsconfig.json\"",
    "start": "node -r module-alias/register ./dist --env=production",
    "start:dev": "nodemon",
    "test": "nodemon --config ./spec/nodemon.json"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "ts-node -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@shared": "dist/shared",
    "@server": "dist/Server"
  },
  "dependencies": {
    "@types/bluebird": "^3.5.32",
    "@types/validator": "^13.1.0",
    "command-line-args": "^5.1.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^3.23.3",
    "http-status-codes": "^1.4.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "pg": "^8.2.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.22.3",
    "sequelize-typescript": "^1.1.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.6",
    "@types/find": "^0.2.1",
    "@types/helmet": "0.0.47",
    "@types/jasmine": "^3.5.11",
    "@types/jsonfile": "^6.0.0",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.14",
    "@types/supertest": "^2.0.10",
    "find": "^0.3.0",
    "fs-extra": "^9.0.1",
    "jasmine": "^3.5.0",
    "jsonfile": "^6.0.1",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.2",
    "typescript": "^3.9.6"
  }
}
